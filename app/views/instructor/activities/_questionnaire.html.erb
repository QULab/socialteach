<div class="panel panel-default">
  <div class="panel-body">
    <% container.questionnaire.m_questions.each_with_index do |question, i| %>
      <div class= "row">
        <div class="col-md-12">
          <div class="well">
            <div class="row">
              <%= question.text %>
            </div>
            <div class="row">
              <% question.answers.each do |answer| %>
                  <%= answer.text %>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
